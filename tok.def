DEF(INCLUDE      ,"include"              ,"(\\s*#\\s*include(?=[^\\w]))"),
DEF(DEFINE       ,"define"               ,"(\\s*#\\s*define(?=[^\\w]))"),    
DEF(UNDEF        ,"undef"                ,"(\\s*#\\s*undef(?=[^\\w]))"),     
DEF(IFDEF        ,"ifdef"                ,"(\\s*#\\s*ifdef(?=[^\\w]))"),
DEF(ENDIF        ,"endif"                ,"(\\s*#\\s*endif(?=[^\\w]))"),

DEF(TOWSHARP     ,"##"                   ,"(##)"),        
DEF(SHARP        ,"#"                    ,"(#)"),         
DEF(SPACE        ," "                    ,"([ \\t]+)"),   
DEF(LINE         ,"\\r\\n"               ,"([\r]?\n)"),   

DEF(ELLIPSIS     ,"..."                  ,"([\\.]{3})"),  
DEF(RIGHT_ASSIGN ,">>="                  ,"(>>=)"),        
DEF(LEFT_ASSIGN  ,"<<="                  ,"(<<=)"),        
DEF(ADD_ASSIGN   ,"+="                   ,"(\\+=)"),         
DEF(SUB_ASSIGN   ,"-="                   ,"(-=)"),
DEF(MUL_ASSIGN   ,"*="                   ,"(\\*=)"),
DEF(DIV_ASSIGN   ,"/="                   ,"(/=)"),
DEF(MOD_ASSIGN   ,"%="                   ,"(%=)"),
DEF(AND_ASSIGN   ,"&="                   ,"(&=)"),
DEF(EQUAL        ,"="                    ,"(=)"),  
DEF(COMMA         ,","                    ,"(,)"),
DEF(LPAREN       ,"("                    ,"(\\()"),
DEF(RPAREN       ,")"                    ,"(\\))"),
DEF(BANG         ,"!"                    ,"(!)"), 
DEF(GT           ,">"                    ,"(>)"),
DEF(LT           ,"<"                    ,"(<)"),
DEF(DOT          ,"."                    ,"(\\.)"),
DEF(SEMI         ,";"                    ,"(;)"),
DEF(IDENT        ,"ident"                ,"([A-Za-z_]+\\w*)"),
DEF(NUMBER       ,"number"               ,"([0-9]+)"),